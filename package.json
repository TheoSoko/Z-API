  "bugs": {
    "url": "https://gitlab.com/TheoSoko/zemus-api/issues"
  },
  "homepage": "https://gitlab.com/TheoSoko/zemus-api#readme",
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.11.18",
    "@types/validator": "^13.7.15",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "validator": "^13.9.0"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.2.1",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@types/boom": "^7.3.2",
    "@types/hapi": "^18.0.8",
    "argon2": "^0.30.3",
    "axios": "^1.4.0",
    "gec": "^1.0.3",
    "jimp": "^0.22.7",
    "jsdom": "^22.0.0",
    "knex": "^2.4.2",
    "mysql": "^2.18.1",
    "pm2": "^5.3.0"
  }
}
